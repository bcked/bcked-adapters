{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module",
		"project": "tsconfig.json"
	},
	"plugins": ["@typescript-eslint"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/strict",
		"plugin:prettier/recommended"
	],
	"env": {
		"es6": true,
		"node": true
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": "error"
	}
}
